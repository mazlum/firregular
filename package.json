{
  "name": "firregular",
  "version": "1.0.0",
  "description": "a simple electron app that allows to find infinitive, simple past and past participle forms of irregular verbs",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack:osx": "electron-packager . firregular --out=dist/osx --platform=darwin --arch=x64 --icon=src/assets/letter.icns",
    "pack:win32": "electron-packager . firregular  --out=dist/win --platform=win32 --arch=ia32 --icon=src/assets/letter.ico",
    "pack:win64": "electron-packager . firregular  --out=dist/win --platform=win32 --arch=x64 --icon=src/assets/letter.ico",
    "pack:linux": "electron-packager . firregular  --out=dist/linux --platform=linux --icon=src/assets/letter.ico",
    "build": "npm run pack:osx && npm run pack:win32 && npm run pack:win64",    
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mazlum AÄŸar",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "electron": "^1.4.15",
    "electron-packager": "^8.5.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "webpack": "^2.2.1"
  }
}
